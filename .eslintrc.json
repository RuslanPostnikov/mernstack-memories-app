{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 12
  },
  "rules": {
    "new-cap": 0,
    "require-jsdoc": 0,
    "max-len": [
      "error",
      {
        "code": 150
      }
    ],
    "linebreak-style": [
      "error",
      "windows"
    ],
    "camelcase": "off",
    "object-curly-spacing": [
      "error",
      "always"
    ]
  }
}
